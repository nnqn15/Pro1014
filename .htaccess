RewriteEngine On
RewriteBase /php2/asm/
# RewriteBase / 
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule ^public/assets/(.*)/(.*)/(.*)$ public/assets/$1/$2/$3 [QSA,L]
RewriteRule ^public/assets/(.*)/(.*)$ public/assets/$1/$2 [QSA,L]
RewriteRule ^public/upload/(.*)$ public/upload/$1 [QSA,L]
RewriteRule ^public/assets/(.*)$ public/assets/$1 [QSA,L]
RewriteRule ^(.*)/(.*)/(.*)$ index.php?url=$1/$2/$3 [QSA,L] 
RewriteRule ^(.*)/(.*)$ index.php?url=$1/$2 [QSA,L]  
